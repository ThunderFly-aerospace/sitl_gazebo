<sdf version='1.5'>


  <model name='autogyro'>
   <pose>0 0 0.2 0 0 0</pose>
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0.025 0 0 0 0 0</pose>
        <mass>0.8</mass>
        <inertia>
          <ixx>0.005333333333333334</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.005333333333333334</iyy>
          <iyz>0</iyz>
          <izz>0.005333333333333334</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <!--dummy  body collision-->
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.2 0.2</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.01</mu>
              <mu2>0.01</mu2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth>0.001</min_depth>
              <kp>1e10</kp>
            </ode>
          </contact>
        </surface>
      </collision>
      <visual name='base_link_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.2 0.2</size>
          </box>
        </geometry>
      </visual>

     <!--ocas - visual-->
     <visual name='tail_connection'>
        <pose>-0.25 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>-0.5 0.01 0.01 </size>
          </box>
        </geometry>
     </visual>
     <visual name='tail_elevator'>
        <pose>-0.55 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.2 0.01 </size>
          </box>
        </geometry>
     </visual>
     <visual name='tail_rudler'>
        <pose>-0.55 0 0.05 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.01 0.1 </size>
          </box>
        </geometry>
     </visual>

	<!-- rotor !-->
	<visual name='leftBlade'>
		  <geometry>
		       <box>
          	      <size> 0.04 0.4 0.01</size>
               </box>
		 </geometry>
	 </visual>
	<visual name='rightBlade'>
		  <geometry>
		       <box>
          	      <size> 0.04 0.4 0.01</size>
               </box>
		 </geometry>
	 </visual>
     <self_collide>0</self_collide>
    </link>

	<plugin name="rotor_plugin" filename="libgazebo_rotor_plugin.so">
		<base_link>base_link</base_link>
		<rotor_pos>0.0 0.0 0.25</rotor_pos><!-- offset from base_link-->
		<blade_length>0.4</blade_length>
        <blade_width>0.04</blade_width>
        <blade_weight>0.04</blade_weight>
        <blade_delta>12</blade_delta>
		<left_blade_visual>leftBlade</left_blade_visual>
		<right_blade_visual>rightBlade</right_blade_visual>
        <element_count>30</element_count>
        <air_density>1.2041</air_density>
        <blade_polar>
            <point angle="0" CL="0.1" CD="0.2"/>
            <point angle="1" CL="1.1" CD="1.2"/>
        </blade_polar>
	</plugin>

    <plugin name="slow" filename="libgazebo_slow_plugin.so">
	      <base_link>base_link</base_link>
	      <coef>0.5</coef>
    </plugin>

    <link name='imu_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
      </inertial>
    </link>
    <joint name='imu_joint' type='revolute'>
      <child>imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

<!--==================================================WHEELS================================================================-->
    <include>
          <uri>model://auto-G2/wheel</uri>
		  <name>center_front_wheel</name>
          <pose> 0.5 0.0 -0.095 1.5707 1.5707</pose> 
    </include>

	<include>
          <uri>model://auto-G2/wheel</uri>
		  <name>left_rear_wheel</name>
          <pose> -0.3 0.3 -0.095 1.5707 1.5707</pose>  
    </include>

	<include>
          <uri>model://auto-G2/wheel</uri>
		  <name>right_rear_wheel</name>
          <pose> -0.3 -0.3 -0.095 1.5707 1.5707</pose>  
    </include>
  
    <joint name="center_front_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>center_front_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>

    <joint name="left_rear_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>left_rear_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>

    <joint name="right_rear_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>right_rear_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>
<!--=================================================== TAIL ===================================================================-->
   <link name="rudder">
      <pose> -0.61 0 0.05 0 0 0 </pose>
      <inertial>
        <mass>0.006</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <iyy>0.00001</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
       <visual name="left_rudder_visual">
        <geometry>
          <box>
            <size>0.010000000000000002 0.01 0.1</size>
          </box>
        </geometry>
      </visual>
    </link>

   <joint name="rudder_joint" type="revolute">
      <parent>base_link</parent>
      <child>rudder</child>
      <pose>0.010000000000000002 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.53</lower>
          <upper>0.53</upper>
        </limit>
      </axis>
    </joint>

    <link name="elevator">
      <pose> -0.61 0 0 0 0 0 </pose>
      <inertial>
        <mass>0.006</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <iyy>0.00001</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
       <visual name="elev_visual">
        <geometry>
          <box>
            <size>0.010000000000000002 0.2 0.01 </size>
          </box>
        </geometry>
      </visual>
    </link>

   <joint name="elev_joint" type="revolute">
      <parent>base_link</parent>
      <child>elevator</child>
      <pose>0.010000000000000002 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.53</lower>
          <upper>0.53</upper>
        </limit>
      </axis>
    </joint>

   <plugin name="elevator_plugin" filename="libLiftDragPlugin.so">
      <a0>0.0</a0>
      <cla>4.752798721</cla>
      <cda>0.6417112299</cda>
      <cma>-1.8</cma>
      <alpha_stall>0.3391428111</alpha_stall>
      <cla_stall>-3.85</cla_stall>
      <cda_stall>-0.9233984055</cda_stall>
      <cma_stall>0</cma_stall>
      <cp>-0.55 0 0 %></cp>
      <area>0.020000000000000004</area>
      <air_density>1.2041</air_density>
      <forward>1 0 0</forward>
      <upward>0 0 1</upward>
      <link_name>base_link</link_name>
      <control_joint_name>elev_joint</control_joint_name>
      <control_joint_rad_to_cl>-4.0</control_joint_rad_to_cl>
    </plugin>-->

    <plugin name="rightrudder" filename="libLiftDragPlugin.so">
      <a0>0.0</a0>
      <cla>4.752798721</cla>
      <cda>0.6417112299</cda>
      <cma>-1.8</cma>
      <alpha_stall>0.3391428111</alpha_stall>
      <cla_stall>-3.85</cla_stall>
      <cda_stall>-0.9233984055</cda_stall>
      <cma_stall>0</cma_stall>
      <cp>-0.55 0 0.05</cp>
      <area>0.010000000000000002</area>
      <air_density>1.2041</air_density>
      <forward>1 0 0</forward>
      <upward>0 1 0</upward>
      <link_name>base_link</link_name>
     <!-- <control_joint_name>
        rudder_joint
      </control_joint_name>
      <control_joint_rad_to_cl>4.0</control_joint_rad_to_cl>-->
    </plugin>


<!--============================================================== VRTULE ==================================================-->
    <joint name='propeller_joint' type='revolute'>
      <child>propeller</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0.9986295347545738 0 -0.05233595624294383</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <link name='propeller'>
      <pose>0.25 0 0.04 0 1.6231562043547265 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-5</iyy>
          <iyz>0</iyz>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='propeller_visual'>
        <geometry>
          <mesh>
            <scale>1 1 1 </scale>
            <uri>model://auto-G2/meshes/iris_prop_ccw.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>

    <plugin name="motor" filename="libgazebo_simple_motor.so">
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>4</motorNumber>
      <base_link>base_link</base_link>
      <force_pos>0.25 0 0.04</force_pos>
      <force_dir>0.9986295347545738 0 -0.05233595624294383</force_dir>
      <coef>0.01</coef>
    </plugin>


<!--==========================================================Joystick for dummy joints============================================-->
<link name='joystic_base_link'>
	<kinematic>1</kinematic>
</link>
   
<joint name='roll_joint' type='revolute'>
      <parent>joystic_base_link</parent>
      <child>joystic_roll_link</child>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
</joint>

<link name='joystic_roll_link'>
	<kinematic>1</kinematic>
</link>

<joint name='pitch_joint' type='revolute'>
      <parent>joystic_roll_link</parent>
      <child>joystic_pitch_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
</joint>

<link name='joystic_pitch_link'>
	<kinematic>1</kinematic>
</link>
<!--========================================================= PX4Interface ================================================-->

    <plugin name='gazebo_imu_plugin' filename='libgazebo_imu_plugin.so'>
      <robotNamespace></robotNamespace>
      <linkName>imu_link</linkName>
      <imuTopic>/imu</imuTopic>
      <gyroscopeNoiseDensity>0.0003394</gyroscopeNoiseDensity>
      <gyroscopeRandomWalk>3.8785e-05</gyroscopeRandomWalk>
      <gyroscopeBiasCorrelationTime>1000.0</gyroscopeBiasCorrelationTime>
      <gyroscopeTurnOnBiasSigma>0.0087</gyroscopeTurnOnBiasSigma>
      <accelerometerNoiseDensity>0.004</accelerometerNoiseDensity>
      <accelerometerRandomWalk>0.006</accelerometerRandomWalk>
      <accelerometerBiasCorrelationTime>300.0</accelerometerBiasCorrelationTime>
      <accelerometerTurnOnBiasSigma>0.196</accelerometerTurnOnBiasSigma>
    </plugin>
    <plugin name="gps_plugin" filename="libgazebo_gps_plugin.so">
        <robotNamespace></robotNamespace>
        <gpsNoise>true</gpsNoise>
    </plugin>

    <plugin name='mavlink_interface' filename='libgazebo_mavlink_interface.so'>
      <robotNamespace></robotNamespace>
      <imuSubTopic>/imu</imuSubTopic>
      <gpsSubTopic>/gps</gpsSubTopic>
      <mavlink_addr>INADDR_ANY</mavlink_addr>
      <mavlink_udp_port>14560</mavlink_udp_port>
      <serialEnabled>false</serialEnabled>
      <serialDevice>/dev/ttyACM0</serialDevice>
      <baudRate>921600</baudRate>
      <qgc_addr>INADDR_ANY</qgc_addr>
      <qgc_udp_port>14550</qgc_udp_port>
      <hil_mode>false</hil_mode>
      <hil_state_level>false</hil_state_level>
      <motorSpeedCommandPubTopic>/gazebo/command/motor_speed</motorSpeedCommandPubTopic>
      <control_channels>

   <channel name="propeller">
      <input_index>4</input_index>
      <input_offset>0</input_offset>
      <input_scaling>750</input_scaling>
      <zero_position_disarmed>0</zero_position_disarmed>
      <zero_position_armed>0</zero_position_armed>
      <joint_control_type>velocity</joint_control_type>
      <!--<joint_control_pid>
        <p>1</p>
        <i>0</i>
        <d>0.001</d>
        <iMax>0</iMax>
        <iMin>0</iMin>
        <cmdMax>5</cmdMax>
        <cmdMin>-1</cmdMin>
      </joint_control_pid>-->
      <joint_name>propeller_joint</joint_name>
    </channel>

    <!-- manuální ovládání placičky-->

      <channel name="roll_channel">
          <input_index>2</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.2</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_name>roll_joint</joint_name>
          <joint_control_type>position_gztopic</joint_control_type>
		  <gztopic>/rotor_roll_cmd</gztopic>
        </channel>

	  <channel name="pitch_channel">
          <input_index>3</input_index>
          <input_offset>0.0</input_offset>
          <input_scaling>0.2</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_name>pitch_joint</joint_name>
          <joint_control_type>position_gztopic</joint_control_type>
		  <gztopic>/rotor_pitch_cmd</gztopic>
       </channel>


