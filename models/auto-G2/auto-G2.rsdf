<sdf version='1.5'>
<%
  # Autogyro parameters

  #body mass and its position - approximation of CenterOfMass 
  body_mass   = 0.8
  body_size_x=0.20
  body_size_y=0.20
  body_size_z=0.2

  body_cog_x = 0.025
  body_cog_y = 0
  body_cog_z = 0.05


  #tail
  #ocas míří ve směru osy x

  tail_pos_x= -0.5
  tail_elev_size_x=0.1
  tail_elev_size_y=0.2
  tail_rudler_size_x=0.1
  tail_rudler_size_z=0.1

  #motor
  motor_x=0.25
  motor_y=0
  motor_z=0.04
  
  motor_pitch_angle=-3.0

  #rotor
  rotor_x = 0.0
  rotor_y = 0.0
  rotor_z = 0.25

  blade_length = 0.4
  blade_width = 0.04
  blade_thin= 0.01
  blade_weight = 0.040
  blade_flap_delta_angle=12

  #wheels position
  front_x = 0.5
  front_z = -0.095

  rear_x = -0.30
  rear_y = 0.30 #jedno je + druhý -
  rear_z = -0.095
 
%>

  <model name='autogyro'>
   <pose>0 0 0.2 0 0 0</pose>
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose><%= body_cog_x %> <%= body_cog_y %> <%= body_cog_y %> 0 0 0</pose>
        <mass><%= body_mass %></mass>
        <inertia>
          <ixx><%= body_mass/12*(body_size_y*body_size_y+body_size_z*body_size_z) %></ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy><%=body_mass/12*(body_size_x*body_size_x+body_size_z*body_size_z) %></iyy>
          <iyz>0</iyz>
          <izz><%=body_mass/12*(body_size_x*body_size_x+body_size_y*body_size_y) %></izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <!--dummy  body collision-->
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size><%=body_size_x%> <%=body_size_y%> <%=body_size_z%></size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.01</mu>
              <mu2>0.01</mu2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth>0.001</min_depth>
              <kp>1e10</kp>
            </ode>
          </contact>
        </surface>
      </collision>
      <visual name='base_link_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size><%=body_size_x%> <%=body_size_y%> <%=body_size_z%></size>
          </box>
        </geometry>
      </visual>

     <!--ocas - visual-->
     <visual name='tail_connection'>
        <pose><%=tail_pos_x/2%> 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size><%=tail_pos_x%> 0.01 0.01 </size>
          </box>
        </geometry>
     </visual>
     <visual name='tail_elevator'>
        <pose><%=tail_pos_x-tail_elev_size_x/2%> 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size><%=tail_elev_size_x%> <%=tail_elev_size_y %> 0.01 </size>
          </box>
        </geometry>
     </visual>
     <visual name='tail_rudler'>
        <pose><%=tail_pos_x-tail_rudler_size_x/2%> 0 <%=tail_rudler_size_z/2%> 0 0 0</pose>
        <geometry>
          <box>
            <size><%=tail_rudler_size_x%> 0.01 <%=tail_rudler_size_z%> </size>
          </box>
        </geometry>
     </visual>

	<!-- rotor !-->
	<visual name='leftBlade'>
		  <geometry>
		       <box>
          	      <size> <%=blade_width%> <%=blade_length%> <%=blade_thin%></size>
               </box>
		 </geometry>
	 </visual>
	<visual name='rightBlade'>
		  <geometry>
		       <box>
          	      <size> <%=blade_width%> <%=blade_length%> <%=blade_thin%></size>
               </box>
		 </geometry>
	 </visual>
     <self_collide>0</self_collide>
    </link>

	<plugin name="rotor_plugin" filename="libgazebo_rotor_plugin.so">
		<base_link>base_link</base_link>
		<rotor_pos><%= rotor_x%> <%= rotor_y%> <%= rotor_z%></rotor_pos><!-- offset from base_link-->
		<blade_length><%=blade_length%></blade_length>
        <blade_width><%=blade_width%></blade_width>
        <blade_weight><%=blade_weight%></blade_weight>
        <blade_delta><%=blade_flap_delta_angle%></blade_delta>
		<left_blade_visual>leftBlade</left_blade_visual>
		<right_blade_visual>rightBlade</right_blade_visual>
        <element_count>30</element_count>
        <air_density>1.2041</air_density>
        <blade_polar>
<point angle="-20" CL="-4" CD="4"/>
<point angle="-19" CL="-3.4295" CD="3.61"/>
<point angle="-18" CL="-2.916" CD="3.24"/>
<point angle="-17" CL="-2.4565" CD="2.89"/>
<point angle="-16" CL="-2.048" CD="2.56"/>
<point angle="-15" CL="-1.6875" CD="2.25"/>
<point angle="-14" CL="-1.372" CD="1.96"/>
<point angle="-13" CL="-1.0985" CD="1.69"/>
<point angle="-12" CL="-0.864" CD="1.44"/>
<point angle="-11" CL="-0.6655" CD="1.21"/>
<point angle="-10" CL="-0.5" CD="1"/>
<point angle="-9" CL="-0.3645" CD="0.81"/>
<point angle="-8" CL="-0.256" CD="0.64"/>
<point angle="-7" CL="-0.1715" CD="0.49"/>
<point angle="-6" CL="-0.108" CD="0.36"/>
<point angle="-5" CL="-0.0625" CD="0.25"/>
<point angle="-4" CL="-0.032" CD="0.16"/>
<point angle="-3" CL="-0.0135" CD="0.09"/>
<point angle="-2" CL="-0.004" CD="0.04"/>
<point angle="-1" CL="-0.0005" CD="0.01"/>
<point angle="0" CL="0" CD="0"/>
<point angle="1" CL="0.0005" CD="0.01"/>
<point angle="2" CL="0.004" CD="0.04"/>
<point angle="3" CL="0.0135" CD="0.09"/>
<point angle="4" CL="0.032" CD="0.16"/>
<point angle="5" CL="0.0625" CD="0.25"/>
<point angle="6" CL="0.108" CD="0.36"/>
<point angle="7" CL="0.1715" CD="0.49"/>
<point angle="8" CL="0.256" CD="0.64"/>
<point angle="9" CL="0.3645" CD="0.81"/>
<point angle="10" CL="0.5" CD="1"/>
<point angle="11" CL="0.6655" CD="1.21"/>
<point angle="12" CL="0.864" CD="1.44"/>
<point angle="13" CL="1.0985" CD="1.69"/>
<point angle="14" CL="1.372" CD="1.96"/>
<point angle="15" CL="1.6875" CD="2.25"/>
<point angle="16" CL="2.048" CD="2.56"/>
<point angle="17" CL="2.4565" CD="2.89"/>
<point angle="18" CL="2.916" CD="3.24"/>
<point angle="19" CL="3.4295" CD="3.61"/>
<point angle="20" CL="4" CD="4"/>
        </blade_polar>
	</plugin>

    <plugin name="slow" filename="libgazebo_slow_plugin.so">
	      <base_link>base_link</base_link>
	      <coef>0.5</coef>
    </plugin>

    <link name='imu_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0</iyz>
          <izz>0.0001</izz>
        </inertia>
      </inertial>
    </link>
    <joint name='imu_joint' type='revolute'>
      <child>imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

<!--==================================================WHEELS================================================================-->
    <include>
          <uri>model://auto-G2/wheel</uri>
		  <name>center_front_wheel</name>
          <pose> <%= front_x %> 0.0 <%= front_z %> 1.5707 1.5707</pose> 
    </include>

	<include>
          <uri>model://auto-G2/wheel</uri>
		  <name>left_rear_wheel</name>
          <pose> <%= rear_x %> <%= rear_y %> <%= rear_z %> 1.5707 1.5707</pose>  
    </include>

	<include>
          <uri>model://auto-G2/wheel</uri>
		  <name>right_rear_wheel</name>
          <pose> <%= rear_x %> <%= -rear_y %> <%= rear_z %> 1.5707 1.5707</pose>  
    </include>
  
    <joint name="center_front_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>center_front_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>

    <joint name="left_rear_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>left_rear_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>

    <joint name="right_rear_wheel_joint" type="revolute">
      <pose>0 0 0 0 0 0 </pose>
      <child>right_rear_wheel::wheel_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <limit>
            <cfm>0.000000</cfm>
            <erp>0.800000</erp>
          </limit>
        </ode>
      </physics>
    </joint>
<!--=================================================== TAIL ===================================================================-->
   <link name="rudder">
      <pose> <%=tail_pos_x-1.1*tail_rudler_size_x %> 0 <%=tail_rudler_size_z/2%> 0 0 0 </pose>
      <inertial>
        <mass>0.006</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <iyy>0.00001</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
       <visual name="left_rudder_visual">
        <geometry>
          <box>
            <size><%=0.1*tail_rudler_size_x%> 0.01 <%=tail_rudler_size_z%></size>
          </box>
        </geometry>
      </visual>
    </link>

   <joint name="rudder_joint" type="revolute">
      <parent>base_link</parent>
      <child>rudder</child>
      <pose><%=0.1*tail_rudler_size_x%> 0 0 0 0 0</pose>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.53</lower>
          <upper>0.53</upper>
        </limit>
      </axis>
    </joint>

    <link name="elevator">
      <pose> <%=tail_pos_x-1.1*tail_elev_size_x %> 0 0 0 0 0 </pose>
      <inertial>
        <mass>0.006</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <iyy>0.00001</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
       <visual name="elev_visual">
        <geometry>
          <box>
            <size><%=0.1*tail_elev_size_x%> <%=tail_elev_size_y%> 0.01 </size>
          </box>
        </geometry>
      </visual>
    </link>

   <joint name="elev_joint" type="revolute">
      <parent>base_link</parent>
      <child>elevator</child>
      <pose><%=0.1*tail_elev_size_x%> 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.53</lower>
          <upper>0.53</upper>
        </limit>
      </axis>
    </joint>

   <plugin name="elevator_plugin" filename="libLiftDragPlugin.so">
      <a0>0.0</a0>
      <cla>4.752798721</cla>
      <cda>0.6417112299</cda>
      <cma>-1.8</cma>
      <alpha_stall>0.3391428111</alpha_stall>
      <cla_stall>-3.85</cla_stall>
      <cda_stall>-0.9233984055</cda_stall>
      <cma_stall>0</cma_stall>
      <cp><%= tail_pos_x-tail_elev_size_x/2 %> 0 0 %></cp>
      <area><%=tail_elev_size_x*tail_elev_size_y %></area>
      <air_density>1.2041</air_density>
      <forward>1 0 0</forward>
      <upward>0 0 1</upward>
      <link_name>base_link</link_name>
      <control_joint_name>elev_joint</control_joint_name>
      <control_joint_rad_to_cl>-4.0</control_joint_rad_to_cl>
    </plugin>-->

    <plugin name="rightrudder" filename="libLiftDragPlugin.so">
      <a0>0.0</a0>
      <cla>4.752798721</cla>
      <cda>0.6417112299</cda>
      <cma>-1.8</cma>
      <alpha_stall>0.3391428111</alpha_stall>
      <cla_stall>-3.85</cla_stall>
      <cda_stall>-0.9233984055</cda_stall>
      <cma_stall>0</cma_stall>
      <cp><%= tail_pos_x-tail_rudler_size_x/2 %> 0 <%= tail_rudler_size_z/2 %></cp>
      <area><%= tail_rudler_size_x*tail_rudler_size_z %></area>
      <air_density>1.2041</air_density>
      <forward>1 0 0</forward>
      <upward>0 1 0</upward>
      <link_name>base_link</link_name>
     <!-- <control_joint_name>
        rudder_joint
      </control_joint_name>
      <control_joint_rad_to_cl>4.0</control_joint_rad_to_cl>-->
    </plugin>


<!--============================================================== VRTULE ==================================================-->
    <joint name='propeller_joint' type='revolute'>
      <child>propeller</child>
      <parent>base_link</parent>
      <axis>
        <xyz><%= Math.cos(motor_pitch_angle/360.0*2.0*Math::PI) %> 0 <%= Math.sin(motor_pitch_angle/360.0*2.0*Math::PI) %></xyz>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <link name='propeller'>
      <pose><%= motor_x %> <%= motor_y %> <%= motor_z%> 0 <%= Math::PI/2-motor_pitch_angle/360.0*2.0*Math::PI %> 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-5</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-5</iyy>
          <iyz>0</iyz>
          <izz>1e-5</izz>
        </inertia>
      </inertial>
      <visual name='propeller_visual'>
        <geometry>
          <mesh>
            <scale>1 1 1 </scale>
            <uri>model://auto-G2/meshes/iris_prop_ccw.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>

    <plugin name="motor" filename="libgazebo_simple_motor.so">
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>4</motorNumber>
      <base_link>base_link</base_link>
      <force_pos><%= motor_x %> <%= motor_y %> <%= motor_z%></force_pos>
      <force_dir><%= Math.cos(motor_pitch_angle/360.0*2.0*Math::PI) %> 0 <%= Math.sin(motor_pitch_angle/360.0*2.0*Math::PI) %></force_dir>
      <coef>0.01</coef>
    </plugin>


<!--==========================================================Joystick for dummy joints============================================-->
<link name='joystic_base_link'>
	<kinematic>1</kinematic>
</link>
   
<joint name='roll_joint' type='revolute'>
      <parent>joystic_base_link</parent>
      <child>joystic_roll_link</child>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
</joint>

<link name='joystic_roll_link'>
	<kinematic>1</kinematic>
</link>

<joint name='pitch_joint' type='revolute'>
      <parent>joystic_roll_link</parent>
      <child>joystic_pitch_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
</joint>

<link name='joystic_pitch_link'>
	<kinematic>1</kinematic>
</link>
<!--========================================================= PX4Interface ================================================-->

    <plugin name='gazebo_imu_plugin' filename='libgazebo_imu_plugin.so'>
      <robotNamespace></robotNamespace>
      <linkName>imu_link</linkName>
      <imuTopic>/imu</imuTopic>
      <gyroscopeNoiseDensity>0.0003394</gyroscopeNoiseDensity>
      <gyroscopeRandomWalk>3.8785e-05</gyroscopeRandomWalk>
      <gyroscopeBiasCorrelationTime>1000.0</gyroscopeBiasCorrelationTime>
      <gyroscopeTurnOnBiasSigma>0.0087</gyroscopeTurnOnBiasSigma>
      <accelerometerNoiseDensity>0.004</accelerometerNoiseDensity>
      <accelerometerRandomWalk>0.006</accelerometerRandomWalk>
      <accelerometerBiasCorrelationTime>300.0</accelerometerBiasCorrelationTime>
      <accelerometerTurnOnBiasSigma>0.196</accelerometerTurnOnBiasSigma>
    </plugin>
    <plugin name="gps_plugin" filename="libgazebo_gps_plugin.so">
        <robotNamespace></robotNamespace>
        <gpsNoise>true</gpsNoise>
    </plugin>

    <plugin name='mavlink_interface' filename='libgazebo_mavlink_interface.so'>
      <robotNamespace></robotNamespace>
      <imuSubTopic>/imu</imuSubTopic>
      <gpsSubTopic>/gps</gpsSubTopic>
      <mavlink_addr>INADDR_ANY</mavlink_addr>
      <mavlink_udp_port>14560</mavlink_udp_port>
      <serialEnabled>false</serialEnabled>
      <serialDevice>/dev/ttyACM0</serialDevice>
      <baudRate>921600</baudRate>
      <qgc_addr>INADDR_ANY</qgc_addr>
      <qgc_udp_port>14550</qgc_udp_port>
      <hil_mode>false</hil_mode>
      <hil_state_level>false</hil_state_level>
      <motorSpeedCommandPubTopic>/gazebo/command/motor_speed</motorSpeedCommandPubTopic>
      <control_channels>

   <channel name="propeller">
      <input_index>4</input_index>
      <input_offset>0</input_offset>
      <input_scaling>750</input_scaling>
      <zero_position_disarmed>0</zero_position_disarmed>
      <zero_position_armed>0</zero_position_armed>
      <joint_control_type>velocity</joint_control_type>
      <!--<joint_control_pid>
        <p>1</p>
        <i>0</i>
        <d>0.001</d>
        <iMax>0</iMax>
        <iMin>0</iMin>
        <cmdMax>5</cmdMax>
        <cmdMin>-1</cmdMin>
      </joint_control_pid>-->
      <joint_name>propeller_joint</joint_name>
    </channel>

    <!-- manuální ovládání placičky-->

      <channel name="roll_channel">
          <input_index>2</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.2</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_name>roll_joint</joint_name>
          <joint_control_type>position_gztopic</joint_control_type>
		  <gztopic>/rotor_roll_cmd</gztopic>
        </channel>

	  <channel name="pitch_channel">
          <input_index>3</input_index>
          <input_offset>0.0</input_offset>
          <input_scaling>0.2</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_name>pitch_joint</joint_name>
          <joint_control_type>position_gztopic</joint_control_type>
		  <gztopic>/rotor_pitch_cmd</gztopic>
       </channel>


        <!-- ocas-->
       <!-- <channel name="rudder_channel">
          <input_index>0</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.8</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_control_type>position</joint_control_type>
          <joint_control_pid>
            <p>1</p>
            <i>0</i>
            <d>0.001</d>
            <iMax>0</iMax>
            <iMin>0</iMin>
            <cmdMax>5</cmdMax>
            <cmdMin>-5</cmdMin>
          </joint_control_pid>
          <joint_name>rudder_joint</joint_name>
        </channel>-->

      </control_channels>
    </plugin>
    <static>0</static>
  </model>
</sdf>
