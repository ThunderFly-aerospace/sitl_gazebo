   <!-- autogyro on moving mox -->
   <model name='moving_box'>
       <include>
          <uri>model://autogyro</uri>
          <pose>0 0 1.2 0 0 0</pose>
       </include>
       <include>
          <uri>model://autogyro/moving_box</uri>
          <name>box</name>
          <pose>0 0 0 0 0 0</pose>
       </include>
       <joint name='car_connection' type='ball'>
          <parent>box::link</parent>
          <child>autogyro::base_link</child>
          <pose>0.1 0 0 0 0 0 </pose>
        </joint>
        <self_collide>1</self_collide>
    </model>



